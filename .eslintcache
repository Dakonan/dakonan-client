\r\n  const username = useSelector(state => state.players.name)\r\n  const roomDetail = useSelector(state => state.rooms.detail)\r\n  const loading = useSelector(state => state.rooms.loading)\r\n  const [turn, setTurn] = useState(false)\r\n  useEffect(() => {\r\n    dispatch(updateGameDetail())\r\n  }, [turn])\r\n\r\n  function clickHandler (i) {\r\n    const board = {...roomDetail.gameState}\r\n    const newState = makeMove(i)(board)\r\n    console.log(i, newState)\r\n    setTurn(!turn)\r\n    dispatch(gameStart(newState, name))\r\n  }\r\n\r\n  return (\r\n    <>\r\n    <NavbarTop username={username}></NavbarTop>\r\n    <div className=\"App\" style={{\r\n      display: 'flex',\r\n      justifyContent: 'center',\r\n      alignItems: 'center',\r\n      position: 'relative',\r\n      top: '5vh',\r\n      // backgroundColor:'blue',\r\n      height: '80vh'\r\n    }}>\r\n      {\r\n        !loading ?\r\n        <h1>Loading</h1>\r\n        :\r\n        roomDetail.name ?\r\n        <>\r\n        {/* <Header /> */}\r\n\r\n        <StatusBar\r\n          player={roomDetail.gameState.player}\r\n          isOver={roomDetail.gameState.isOver}\r\n          message={roomDetail.gameState.message}\r\n        />\r\n{/* \r\n        <div className=\"decoration\">\r\n          <img src={decoration} />\r\n        </div> */}\r\n\r\n        <div className=\"fullPageImage\" style={{\r\n          // backgroundColor: 'red'\r\n        }}>\r\n          <img src={fullPageImage} style={{\r\n            maxWidth: '1095px',\r\n            height: '90vh',\r\n            width: '200vw '\r\n          }}/>\r\n        </div>\r\n\r\n        <Board\r\n          board={roomDetail.gameState.board}\r\n          clickHandler={clickHandler}\r\n        />\r\n\r\n        {/* <button onClick={resetHandler}>\r\n          Reset\r\n        </button> */}\r\n        </>\r\n        :\r\n        <h1>waiting for player 2</h1>\r\n      }\r\n    </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default GamePage;","D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\utils.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\redux\\actions\\index.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\Navbar.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\Header.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\ModalLogin.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\StatusBar.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\RoomList.js",[],"D:\\Hendri\\h8\\p3\\dakonan\\dakonan-client\\src\\components\\Board.js",["140","141"],"import { set } from 'lodash'\r\nimport React, { useEffect, useState } from 'react'\r\nimport { BigHole, Hole } from '.'\r\n\r\nconst Board = ({ board, clickHandler }) => {  \r\n  const [localBoard, setLocalBoard] = useState(board)\r\n  const [light, setLight] = useState([]) //array of index where holes have white background\r\n\r\n  useEffect(() => {\r\n    setLocalBoard(board)\r\n  }, [board])\r\n\r\n  const pebblesAdder = (clickedIndex, currentIndex) => {\r\n    if (clickedIndex < 6) {\r\n      setLocalBoard(localBoard => \r\n        localBoard.map((num, i) => {\r\n          if (i === currentIndex && i !== 13) return num + 1\r\n          return num\r\n        })\r\n      )\r\n    } else if (clickedIndex > 6) {\r\n      setLocalBoard(localBoard =>\r\n        localBoard.map((num, i) => {\r\n          if (i === currentIndex && i !== 6) {\r\n            return num + 1\r\n          }\r\n          return num\r\n        })\r\n      )\r\n    }\r\n  }\r\n\r\n  const lightController = (nextIndex, number) => {\r\n    const index = nextIndex - 1\r\n\r\n    const timeout = setInterval(() => {\r\n      if (number <= 0) {\r\n        setLight([])\r\n        clickHandler(index) //request to server\r\n        clearInterval(timeout)\r\n      } else if (number === 1) {\r\n        if (couldHijack(index, nextIndex)) {\r\n          const homeIndex = index < 6 ? 6 : 13\r\n          cons